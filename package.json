{
  "name": "cloudbrowsers",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "dotenv": "^17.2.2",
      "zod": "^4.1.13",
      "typescript": "^5",
      "@types/node": "^22.13.11",
      "@cloudflare/workers-types": "^4.20251213.0",
      "better-auth": "^1.4.7",
      "alchemy": "^0.81.2",
      "tsdown": "^0.16.5",
      "wrangler": "^4.54.0"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "check-types": "turbo check-types",
    "dev:server": "turbo -F server dev",
    "dev:tester": "turbo -F tester dev",
    "db:push": "turbo -F @cloudbrowsers/db db:push",
    "db:generate": "turbo -F @cloudbrowsers/db db:generate",
    "cleanup": " docker ps -q --filter 'name=workerd-cloudbrowsers' | xargs docker kill",
    "logs": "turbo -F api-worker logs"
  },
  "dependencies": {
    "dotenv": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "turbo": "^2.6.3",
    "typescript": "catalog:",
    "@types/node": "catalog:",
    "@cloudflare/workers-types": "catalog:"
  },
  "packageManager": "bun@1.3.3"
}
